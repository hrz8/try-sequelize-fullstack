{
  "name": "try-sequelize-fullstack",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "db:create": "sequelize db:create --config=sequelize-cli.config.json",
    "db:drop": "sequelize db:drop --config=sequelize-cli.config.json",
    "db:migration:create": "sequelize migration:generate --config=sequelize-cli.config.json",
    "db:migrate": "sequelize db:migrate --config=sequelize-cli.config.json",
    "db:migrate:undo": "sequelize db:migrate:undo --config=sequelize-cli.config.json",
    "db:seed:all": "sequelize db:seed:all --config=sequelize-cli.config.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^2.0.16",
    "sequelize-cli": "^6.4.1"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.20.1"
  }
}
